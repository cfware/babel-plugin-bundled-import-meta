{
	"name": "babel-plugin-bundled-import-meta",
	"version": "0.1.0",
	"description": "Babel plugin to rewrite import.meta for bundled usage",
	"main": "index.js",
	"scripts": {
		"test": "xo && nyc ava -s"
	},
	"engines": {
		"node": ">=8"
	},
	"author": "Corey Farrell",
	"license": "BSD-3-Clause",
	"files": [
		"index.js",
		"fixtures"
	],
	"keywords": [
		"babel",
		"esm",
		"browser",
		"import.meta"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/babel-plugin-bundled-import-meta.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/babel-plugin-bundled-import-meta/issues"
	},
	"homepage": "https://github.com/cfware/babel-plugin-bundled-import-meta#readme",
	"dependencies": {
		"@babel/plugin-syntax-import-meta": "^7.0.0",
		"@babel/template": "^7.0.0"
	},
	"peerDependencies": {
		"@babel/core": "^7.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"ava": "^1.0.0-rc.1",
		"nyc": "^13.1.0",
		"xo": "^0.23.0"
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"include": [
			"*.js",
			"!test.js"
		]
	},
	"xo": {
		"rules": {
			"quotes": [
				"error",
				"single",
				{
					"avoidEscape": true
				}
			]
		}
	}
}
